{
  "expo": {
    "name": "Aora",
    "scheme": "aora",
    "slug": "Aora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "supportsRTL": true,
    "ios": {
      "supportsRTL": true,
      "supportsTablet": true,
      "package": "com.amr.aora"
    },
    "android": {
      "supportsRTL": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Aora to access your photos."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Aora to access your photos to upload images."
        }
      ],
      [
        "expo-document-picker",
        {
          "photosPermission": "Allow Aora to access your photos to upload images."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "supportsRTL": true
    }
  }
}
